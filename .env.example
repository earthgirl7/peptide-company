# Airtable Configuration
# Get these values from your Airtable account

# Your Airtable Personal Access Token
# Create one at: https://airtable.com/create/tokens
AIRTABLE_API_KEY=your_airtable_api_key_here

# Your Airtable Base ID
# Find this in the URL when viewing your base (starts with "app")
AIRTABLE_BASE_ID=your_base_id_here

# The name of your table (default: "Applications")
AIRTABLE_TABLE_NAME=Applications
